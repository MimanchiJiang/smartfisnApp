.box {
  display: flex;
  flex-direction: column;
  height: 100vh;
}
.box .TopBar {
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10px;
}
.box .TopBar .logo {
  height: 100%;
  width: 110rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.box .TopBar .connectStatus {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.box .TopBar .connectStatus .mqtt-status-greenCircle {
  width: 20px;
  height: 20px;
  border-radius: 50%;
  background: green;
  background-image: radial-gradient(lime, transparent);
  background-size: 5px 5px;
}
.box .TopBar .connectStatus .mqtt-status-redCircle {
  width: 20px;
  height: 20px;
  border-radius: 50%;
  background: #dd0303;
  background-image: radial-gradient(#ff9900, transparent);
  background-size: 5px 5px;
}
.box .TopBar .reflash {
  height: 100%;
}
.box .showData {
  height: 600rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.box .showData .showDataQuality {
  width: 90%;
  height: 50%;
}
.box .showData .showDataTemp {
  width: 90%;
  height: 50%;
}
.box .control {
  width: 750rpx;
  flex: 1;
  display: flex;
  justify-content: space-around;
}
.box .control .controlAuto {
  border-radius: 4px;
  width: 365rpx;
  box-shadow: rgba(0, 0, 0, 0.08) 0px 0px 3px 1px;
}
.box .control .controlManual {
  border-radius: 4px;
  box-shadow: rgba(0, 0, 0, 0.08) 0px 0px 3px 1px;
  width: 365rpx;
  height: 100%;
}